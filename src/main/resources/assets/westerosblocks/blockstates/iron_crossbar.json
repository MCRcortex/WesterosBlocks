{
  "multipart": [
    {
      "when": {
        "OR": [
          {
            "north": true
          },
          {
            "north": false,
            "south": false,
            "west": false,
            "east": false
          }
        ]
      },
      "apply": {
        "model": "westerosblocks:block/iron_crossbar_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "OR": [
          {
            "east": true
          },
          {
            "north": false,
            "south": false,
            "west": false,
            "east": false
          }
        ]
      },
      "apply": {
        "model": "westerosblocks:block/iron_crossbar_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "OR": [
          {
            "south": true
          },
          {
            "north": false,
            "south": false,
            "west": false,
            "east": false
          }
        ]
      },
      "apply": {
        "model": "westerosblocks:block/iron_crossbar_side_alt",
        "uvlock": true
      }
    },
    {
      "when": {
        "OR": [
          {
            "west": true
          },
          {
            "north": false,
            "south": false,
            "west": false,
            "east": false
          }
        ]
      },
      "apply": {
        "model": "westerosblocks:block/iron_crossbar_side_alt",
        "y": 90,
        "uvlock": true
      }
    }
  ]
}