{
  "multipart": [
    {
      "apply": [
        {
          "model": "westerosblocks:block/reach_brick_fence_post"
        }
      ]
    },
    {
      "when": {
        "north": "true"
      },
      "apply": [
        {
          "model": "westerosblocks:block/reach_brick_fence_side",
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "east": "true"
      },
      "apply": [
        {
          "model": "westerosblocks:block/reach_brick_fence_side",
          "y": 90,
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "south": "true"
      },
      "apply": [
        {
          "model": "westerosblocks:block/reach_brick_fence_side",
          "y": 180,
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "west": "true"
      },
      "apply": [
        {
          "model": "westerosblocks:block/reach_brick_fence_side",
          "y": 270,
          "uvlock": true
        }
      ]
    }
  ]
}